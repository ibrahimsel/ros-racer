{
    "metadata": {
        "name": "racecar-fleet"
    },
    "spec": {
        "displayName": "racecar-fleet",
        "forceRedeploy": true,
        "components": [
            {
                "name": "muto",
                "type": "muto-agent",
                "properties": {
                }
            }
        ],
        "topologies": [
